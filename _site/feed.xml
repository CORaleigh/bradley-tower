					{
						ev.preventDefault();
						gauge.value(between(0, 1000));
					});
					$(&quot;#gauge-2-res&quot;).on(&#39;click&#39;, function(ev)
					{
						ev.preventDefault();
						gauge.value(0);
					});
				});
				&lt;/script&gt;
				&lt;div id=&quot;gauge-6&quot; style=&quot;height: 250px; margin: 0 30px;&quot;&gt;&lt;/div&gt;
			&lt;/div&gt;
		&lt;/div&gt;
	&lt;/div&gt;
